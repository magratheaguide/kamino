{
    "author": "Daine (https://daine.dev)",
    "bugs": {
        "url": "https://github.com/rp-magrathea/kamino/issues"
    },
    "dependencies": {
        "@cloudfour/simple-svg-placeholder": "^1.0.2",
        "@frctl/fractal": "^1.3.0",
        "@frctl/handlebars": "^1.2.0",
        "@frctl/mandelbrot": "^1.4.0",
        "autoprefixer": "^9.8.6",
        "casual": "^1.6.2",
        "postcss-cli": "^7.1.2",
        "postcss-extend-rule": "^3.0.0",
        "postcss-import": "^12.0.1"
    },
    "description": "Setup template for forum roleplay theming projects",
    "devDependencies": {
        "@rp-magrathea/stylelint-alphabetical-clockwise-order": "git://github.com/rp-magrathea/stylelint-alphabetical-clockwise-order.git#v1.0.0",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^6.11.0",
        "htmlhint": "^0.14.1",
        "prettier": "^2.1.2",
        "stylelint": "^13.7.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-no-unsupported-browser-features": "^4.0.0",
        "stylelint-order": "^4.1.0"
    },
    "homepage": "https://github.com/rp-magrathea/kamino#readme",
    "keywords": [
        "jcink",
        "theme",
        "forum-rp",
        "rp"
    ],
    "name": "kamino",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rp-magrathea/kamino.git"
    },
    "scripts": {
        "build": "npm run-script prettier-write && npm run-script postcss && npm run-script fractal-build",
        "dev-watch": "npm run-script postcss-watch & npm run-script fractal-watch",
        "fractal-build": "fractal build",
        "fractal-watch": "fractal start --sync",
        "lint": "npm run-script lint:css-error ; npm run-script lint:html ; npm run-script lint:js",
        "lint:css": "npx stylelint --config tools/.stylelintrc.yml --ignore-path tools/.stylelintignore \"{src,dist}/**/*.{css,html,js}\" --allow-empty-input",
        "lint:css-error": "npm run-script lint:css -- --quiet || echo 'failures found'",
        "lint:css-warning": "npm run-script lint:css || echo 'failures found'",
        "lint:html": "npx htmlhint --config tools/.htmlhintrc \"{src,dist}/**/*.{html,hbs,js}\" || echo 'failures found'",
        "lint:js": "npx eslint --config tools/.eslintrc.yml \"dist/**/*.js\" || echo 'failures found'",
        "postcss": "npx postcss --config tools src/components/stylesheet.css -o dist/assets/stylesheet.css",
        "postcss-watch": "npm run-script postcss -- --watch",
        "prettier-check": "npx prettier --config tools/.prettierrc.yml --ignore-path tools/.prettierignore --check .",
        "prettier-write": "npx prettier --config tools/.prettierrc.yml --ignore-path tools/.prettierignore --write .",
        "stylelint-check": "stylelint-config-prettier-check || echo ",
        "target-browsers": "npx browserslist"
    },
    "version": "1.2.0"
}
